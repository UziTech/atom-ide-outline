{
  "name": "atom-ide-outline",
  "main": "./dist/main",
  "version": "1.6.0",
  "description": "Outline UI compatible with IDE packages",
  "repository": "https://github.com/atom-ide-community/atom-ide-outline",
  "keywords": [
    "atom-package",
    "atom-ide",
    "atom-outline",
    "ide-outline",
    "outline"
  ],
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "atom --test spec",
    "clean": "shx rm -rf dist",
    "babel": "npm run clean && shx cp -r src dist && env NODE_ENV=development env BABEL_ENV=development babel dist --out-dir dist",
    "dev": "npm run clean && env NODE_ENV=development env BABEL_ENV=production rollup -c -w",
    "build": "npm run clean && env NODE_ENV=production env BABEL_ENV=production rollup -c ",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u",
    "prepare": "npm run build"
  },
  "atomTestRunner": "./spec/runner",
  "dependencies": {},
  "devDependencies": {
    "@types/atom": "latest",
    "@types/node": "^14.0.23",
    "typescript": "^3.9.7",
    "tslib": "^2.0.0",
    "jasmine": "^3.5.0",
    "@types/jasmine": "^3.5.11",
    "atom-jasmine3-test-runner": "^4.4.8",
    "prettier": "^2.0.5",
    "eslint": "7.4.0",
    "eslint-config-atomic": "^0.1.0",
    "tslint": "latest",
    "tslint-config-prettier": "latest",
    "rollup": "^2.21.0",
    "rollup-plugin-atomic": "^1.0.2",
    "shx": "latest",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "babel-preset-atomic": "latest",
    "npm-check-updates": "latest",
    "build-commit": "latest"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "outline-view": {
      "versions": {
        "0.1.0": "consumeOutlineProvider"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    }
  },
  "package-deps": {}
}
